  "scripts": {
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "web-app:staging": "npx env-cmd -f .env.staging nx serve web-app",
    "all": "npx nx run-many --target=serve --all --maxParallel=100",
    "prisma-local:format": "dotenv -e .env.local -- npx prisma format",
    "prisma-local:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "prisma-local:db-seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma-local:db-pull": "dotenv -e .env.local -- npx prisma db pull",
    "prisma-local:apply": "npm run prisma-local:format && npm run prisma-local:migrate",
    "build-authentication-service-serverless": "nx build authentication-service --skip-nx-cache --configuration=lambda-serverless",
    "build-user-service-serverless": "nx build user-service --skip-nx-cache --configuration=lambda-serverless",
    "build-serverless-files-authentication-service": "nx build-serverless-files  authentication-service --stage=${npm_config_stage} --verbose",
    "build-serverless-files-user-service": "nx build-serverless-files  user-service --stage=${npm_config_stage} --verbose"
  },
